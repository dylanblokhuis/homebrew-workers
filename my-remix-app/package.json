{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "dev": "cross-env NODE_ENV=development remix build && run-p dev:*",
    "dev:node": "cross-env NODE_ENV=development nodemon ./build/index.js --watch ./build/index.js",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "bundle": "yarn build && webpack && cp ./dist/main.js ../some-app/main.js && rm -rf ../some-app/public && cp -r ./public ../some-app/public",
    "postinstall": "remix setup cloudflare-pages"
  },
  "dependencies": {
    "@remix-run/cloudflare-pages": "^1.2.3",
    "@remix-run/react": "^1.2.3",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3",
    "webpack": "^5.70.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@remix-run/server-runtime": "^1.2.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "esbuild": "^0.14.25",
    "eslint": "^8.9.0",
    "mime": "^3.0.0",
    "typescript": "^4.5.5",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">=14"
  }
}
